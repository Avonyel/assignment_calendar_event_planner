<h1>Users Index</h1>

<a href="/user/new"><button>New User</button></a>

<table>
	<thead>
		<tr>
			<th>First Name</th>
			<th>Last Name</th>
			<th>Username</th>
			<th>Email</th>
			<th>Edit</th>
			<th>Delete</th>
		</tr>
	</thead>
	<tbody>
	{{#each users as |user|}}
		<tr>
			<td>{{user.fname}}</td>
			<td>{{user.lname}}</td>
			<td>{{user.username}}</td>
			<td>{{user.email}}</td>
			<td><a>Edit</a></td>
			<td><a>Delete</a></td>
		</tr>
	{{/each}}
	</tbody>
</table>